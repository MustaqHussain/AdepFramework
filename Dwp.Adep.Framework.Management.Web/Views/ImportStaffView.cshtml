@model IEnumerable<Dwp.Adep.Framework.Management.Web.Models.ImportStaffModel>

@{
    ViewBag.Title = "ImportStaffView";
}

<h2>ImportStaffView</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
      <th>
            Code
        </th>
        <th>
            StaffNumber
        </th>
        <th>
            LastName
        </th>
        <th>
            FirstName
        </th>
        <th>
            NetworkLastName
        </th>
        <th>
            NetworkFirstName
        </th>
        <th>
            GradeID
        </th>
        <th>
            TeamID
        </th>
        <th>
            CommandID
        </th>
        <th>
            LocationID
        </th>
        <th>
            NonIPC
        </th>
        <th>
            LeftIPC
        </th>
        <th>
            RowIdentifier
        </th>
          <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
      <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StaffNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NetworkLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NetworkFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GradeID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeamID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommandID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LocationID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NonIPC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LeftIPC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RowIdentifier)
        </td>
          <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Code }) |
            @Html.ActionLink("Details", "Details", new { id=item.Code }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Code })
        </td>
    </tr>
}

</table>
